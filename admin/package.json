{
  "name": "sns-deepfake",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "sync": "yarn install --frozen-lockfile",
    "dev": "set VITE_MY_ENV=dev && vite --host",
    "build": "vite build",
    "start": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0 --fix",
    "prettier": "prettier --check \"{src,test}/**/*.{js,jsx}\"",
    "prettier:fix": "prettier --write \"{src,test}/**/*.{js,jsx}\"",
    "prepare": "cd .. && husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.9",
    "@tanstack/react-query-devtools": "^5.28.10",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^10.0.5",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.28.6",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "antd": "^5.15.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "vite": "^5.2.0"
  },
  "imports": {
    "#/*": "./src/*"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.20.0",
    "npm": "please-use-yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cd admin && yarn lint-staged"
    }
  }
}
